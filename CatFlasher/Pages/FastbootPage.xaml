<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CatFlasher.FastbootPage"
             Title="FastbootPage">
    <AbsoluteLayout>
        <Grid AbsoluteLayout.LayoutFlags="PositionProportional" AbsoluteLayout.LayoutBounds="0.5,0.5">
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <Entry x:Name="entry" Grid.Column="0" WidthRequest="50"/>
            <Button Text="Execute" Grid.Column="1" Clicked="Button_Clicked"/>
        </Grid>
    </AbsoluteLayout>
</ContentPage>